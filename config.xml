<?xml version='1.0' encoding='utf-8'?>
<widget id="br.eteczl.pam1" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>br.eteczl.pam1</name>
    <description>
        proj pra aula de cordova sobre geolocalizacao
    </description>
    <author email="carolyn.cruz@etec.sp.gov.br" href="http://cordova.io">
        carolyn.cruz@etec.sp.gov.br
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
